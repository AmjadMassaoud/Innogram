{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json",
    "plugins": ["@nestjs/swagger"]
  },
  "monorepo": true,
  "root": "apps/Innogram",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },

    "config-lib": {
      "type": "library",
      "root": "libs/config-lib",
      "entryFile": "index",
      "sourceRoot": "libs/config-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config-lib/tsconfig.lib.json"
      }
    },
    "entities-lib": {
      "type": "library",
      "root": "libs/entities-lib",
      "entryFile": "index",
      "sourceRoot": "libs/entities-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entities-lib/tsconfig.lib.json"
      }
    }
  }
}
